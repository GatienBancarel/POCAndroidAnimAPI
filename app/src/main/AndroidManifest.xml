<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pocandroidanimapi">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ActivitySceneTransitionManager"></activity>
        <activity android:name=".ActivityArgbEvaluator" />
        <activity android:name=".ActivityAlpha" />
        <activity android:name=".ActivityRotate" />
        <activity android:name=".ActivityAnimatorSet" />
        <activity android:name=".ActivityScalling" />
        <activity android:name=".ActivitySpringAnimation" />
        <activity android:name=".ActivityTranslation" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>